{
  "collectCoverage": true,
  "coverageReporters": [
    "text"
  ],
  "rootDir": "./",
  "moduleNameMapper": {
    "^.+\\.(css|scss|sass|svg)$": "identity-obj-proxy"
  },
  "modulePaths": [
    "<rootDir>"
  ],
  "testRegex": "\\.spec\\.jsx?$",
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  }
}
